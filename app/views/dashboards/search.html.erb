<div class="row search-result">
  <div class="col-md-10">
    <h2>Search results</h2>
    <%= will_paginate @athletes, renderer: BootstrapPagination::Rails, class: 'pull-left paginate' %>
  </div>
</div>

<div class="row">
  <% @athletes.each do |athlete| %>
    <div class="col-md-10 well member">
      <%= athlete.full_name %>
    </div>
  <% end %>
</div>